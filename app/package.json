{
  "name": "egg",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.5",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "buffer": "^6.0.3",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "react": "^18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-linkify-it": "^1.0.7",
    "react-photo-view": "^1.2.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "react-swipeable": "^7.0.0",
    "typescript": "^5.2.0",
    "web-vitals": "^3.0.4"
  },
  "scripts": {
    "start": "yarn preprocess-php && react-scripts start",
    "build": "yarn clean-preprocess && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext '.js,.ts,.tsx' src/",
    "preprocess-php": "yarn preprocess-photos",
    "preprocess-photos": "cd public/php && php getPhotos.php > getPhotos.json",
    "clean-preprocess": "rm -f public/php/*.json",
    "pretty": "yarn prettier --write .",
    "pretty-check": "yarn prettier --check .",
    "prepare": "cd .. && husky install app/.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11 ",
    "@prettier/plugin-php": "^0.21.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": ">=6",
    "lint-staged": ">=15.1.0",
    "prettier": "^3.1.0"
  },
  "lint-staged": {
    "*.(js|ts|tsx)": "yarn lint",
    "*": "prettier --write"
  }
}
